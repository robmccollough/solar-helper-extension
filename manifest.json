{
    "name": "Sunpower-Zoho-Transfer",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Pass data between sunpower and zoho",
    "permissions": [
        "tabs", "https://sunpower1.force.com/apex/SPCommunityCustomer#/*", "https://crm.zoho.com/crm/*",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "sun_logo.png",
        "128": "sun_logo.png"
    }

    }